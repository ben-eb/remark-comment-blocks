{
  "name": "remark-comment-blocks",
  "version": "2.0.1",
  "description": "Wrap Markdown with a comment block.",
  "main": "dist/index.js",
  "files": [
    "LICENSE-MIT",
    "dist"
  ],
  "scripts": {
    "report": "nyc report --reporter html",
    "pretest": "eslint src",
    "prepublish": "del-cli dist && BABEL_ENV=publish babel src --out-dir dist --ignore /__tests__/",
    "test": "nyc ava src/__tests__",
    "test-012": "nyc ava src/__tests__"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.6",
    "del-cli": "^0.2.0",
    "eslint": "^3.0.0",
    "eslint-config-cssnano": "^3.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.10.2",
    "nyc": "^8.0.0",
    "remark": "^5.0.1"
  },
  "dependencies": {
    "trim-right": "^1.0.1"
  },
  "keywords": [
    "code",
    "comments",
    "remark",
    "markdown"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ben-eb/remark-comment-blocks",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/remark-comment-blocks",
  "ava": {
    "require": "babel-register"
  },
  "eslintConfig": {
    "extends": "cssnano"
  }
}
